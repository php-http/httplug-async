{
    "name": "php-http/httplug-async",
    "description": "HTTPlug Async",
    "license": "MIT",
    "keywords": ["client", "async", "promise", "http"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "HTTPlug Collaborators"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "henrikbjorn/phpspec-code-coverage" : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Http\\Client\\Async\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpspec run",
        "test-ci": "vendor/bin/phpspec run -c phpspec.ci.yml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
